{
    "name": "@mzawadie/sdk",
    "version": "1.0.0",
    "description": "This package contains all queries and mutations that are used in our sample storefront. It can be used for semi-custom or fully-custom (with ability to extend existing queries) storefront solutions.",
    "homepage": "https://mzawadie.co.tz/",
    "main": "./lib/index.js",
    "type": "commonjs",
    "typings": "./lib/index.d.ts",
    "license": "BSD-3-Clause",
    "engines": {
        "node": ">=12.16.3"
    },
    "keywords": [
        "mzawadie",
        "sdk",
        "storefront",
        "shop",
        "typescript",
        "apollo",
        "graphql"
    ],
    "author": "Emmanuel Maro <emmanuelmichaelmaro733@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git://github.com/Emmanuelmichaelmaro/mzawadie-sdk.git"
    },
    "browser": {
        "fs": false,
        "path": false,
        "process": false,
        "os": false
    },
    "scripts": {
        "build": "tsc",
        "clean": "rimraf lib",
        "build-types": "apollo codegen:generate --target=typescript gqlTypes && rm -rf src/gqlTypes && move-file gqlTypes src/gqlTypes && jscodeshift -t scripts/fixImportPaths.ts src/**/gqlTypes/* --parser=ts",
        "lint": "eslint \"src/**/*.@(tsx|ts|jsx|js)\" --fix && prettier --check \"src/**/*.@(tsx|ts|jsx|js)\" --write",
        "prepare": "npm run clean && npm run build",
        "release": "np",
        "start": "tsc --noEmit --watch",
        "test": "jest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run build"
        }
    },
    "lint-staged": {
        "*.{js,ts}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "apollo": "^2.27.4",
        "apollo3-cache-persist": "^0.13.0",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21"
    },
    "devDependencies": {
        "@apollo/client": "^3.4.17",
        "@apollo/link-error": "^2.0.0-beta.3",
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@pollyjs/adapter-node-http": "^5.1.1",
        "@pollyjs/core": "^5.1.1",
        "@pollyjs/persister-fs": "^5.1.1",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^12.1.2",
        "@trivago/prettier-plugin-sort-imports": "^3.1.1",
        "@types/apollo-upload-client": "^14.1.0",
        "@types/jest": "^27.0.2",
        "@types/jscodeshift": "^0.11.2",
        "@types/lodash": "^4.14.176",
        "@types/pollyjs__adapter-node-http": "^2.0.1",
        "@types/pollyjs__persister-fs": "^2.0.1",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/setup-polly-jest": "^0.5.1",
        "@types/webappsec-credential-management": "^0.6.2",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "apollo-upload-client": "^16.0.0",
        "babel-jest": "^27.3.1",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^19.0.0",
        "eslint-config-airbnb-typescript": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "graphql": "^15.7.2",
        "husky": "^7.0.4",
        "isomorphic-unfetch": "^3.1.0",
        "jest": "^27.3.1",
        "jscodeshift": "^0.13.0",
        "lint-staged": "^11.2.6",
        "move-file-cli": "^3.0.0",
        "node-fetch": "^3.1.0",
        "np": "^7.5.0",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-test-render": "^1.1.2",
        "rimraf": "^3.0.2",
        "setup-polly-jest": "^0.10.0",
        "ts-jest": "^27.0.7",
        "typescript": "^4.4.4"
    },
    "peerDependencies": {
        "graphql": "^15.7.2",
        "@apollo/client": "^3.4.17",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
